<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gymappandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GymAppAndroid">
        <activity
            android:name=".CustomTextField"
            android:exported="false"
            android:label="@string/title_activity_custom_text_field"
            android:theme="@style/Theme.GymAppAndroid.NoActionBar" />
        <activity
            android:name=".CustomEditTextComposable"
            android:exported="false"
            android:label="@string/title_activity_custom_edit_text_composable"
            android:theme="@style/Theme.GymAppAndroid.NoActionBar" />
        <activity
            android:name=".Colors"
            android:exported="true"
            android:label="@string/title_activity_colors"
            android:theme="@style/Theme.GymAppAndroid.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.GymAppAndroid.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>